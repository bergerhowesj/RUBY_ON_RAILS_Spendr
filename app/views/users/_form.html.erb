<div class="info form">
  <%= form_for(@user) do |f| %>
    <%= f.label :username, class: "label"%>
    <%= f.text_field :username, class: 'input' %><br>

    <%= f.label :email, class: "label"%>
    <%= f.text_field :email, class: 'input' %><br>

    <%= f.label :first_name, class: "label"%>
    <%= f.text_field :first_name, class: 'input' %><br>

    <%= f.label :last_name, class: "label"%>
    <%= f.text_field :last_name, class: 'input' %><br>

    <%= f.label :contact_number, class: "label"%>
    <%= f.text_field :contact_number, class: 'input' %><br>

    <%= f.label :unit_number, class: "label"%>
    <%= f.text_field :address_unit_number, class: 'input' %><br>

    <%= f.label :street_number, class: "label"%>
    <%= f.text_field :address_street_number, class: 'input' %><br>

    <%= f.label :street_name, class: "label"%>
    <%= f.text_field :address_street_name, class: 'input' %><br>

    <%= f.label :suburb, class: "label"%>
    <%= f.text_field :address_suburb, class: 'input' %><br>

    <%= f.label :city, class: "label"%>
    <%= f.text_field :address_city, class: 'input' %><br>

    <%= f.label :state, class: "label"%>
    <%= f.text_field :address_state, class: 'input' %><br>

    <%= f.label :postcode, class: "label"%>
    <%= f.number_field :address_postcode, class: 'input' %><br>

    <%= f.label :country, class: "label"%>
    <%= f.text_field :address_country, class: 'input' %><br>

    <%= f.label :password, class: "label"%>
    <%= f.text_field :password, class: 'input'  %><br>

    <%= f.label :Reenter_password, class: "label"%>
    <%= f.text_field :password_confirmation, class: 'input'  %><br>
    <%= f.submit class: "submit" %>
  <% end %>
</div>
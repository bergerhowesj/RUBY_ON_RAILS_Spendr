<div class="info form">
  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :username, class: "label"%>
    <%= f.text_field :username, class: "input"%>

    <%= f.label :password, class: "label"%>
    <%= f.password_field :password, class: "input"%>
    
    <%= f.submit "Log In", class: "submit" %>
  <% end %>
</div>